<aura:component implements = "force:hasRecordId,flexipage:availableForAllPageTypes">
    <aura:attribute name="recordId" type="String" />
    <lightning:card>
        <div  class="slds-page-header slds-page-header_record-home">
            <div class="slds-summary-detail__title">
                <div class="slds-up-assistant__step-summary">
                    <div class="slds-media">
                        <div class="slds-media__body slds-m-top_x-small">
                            <div class="slds-media">
                                <div class="slds-setup-assistant__step-summary-content slds-media__body">
                                    <h3 class="slds-setup-assistant__step-summary-title slds-text-heading_small">
                                        
                                    </h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="slds-page-header__row slds-page-header__row_gutters" >
                <div class="slds-page-header__col-details">
                    <ul class="slds-page-header__detail-row">
                        <li class="slds-page-header__detail-block">
                            <div class="slds-text-title slds-truncate" title="Account Name">Data Inizio</div>
                           	<lightning:formattedDateTime value="" year="numeric" month="numeric" day="numeric" hour="2-digit"
                                 minute="2-digit"  hour12="false"/>
                        </li>
                        <li class="slds-page-header__detail-block">
                            <div class="slds-text-title slds-truncate" title="Account Name">Account Name</div>
                            <div class="slds-truncate" title="Account Name"></div>
                        </li>
                        <li class="slds-page-header__detail-block">
                            <div class="slds-text-title slds-truncate" title="Codice Fiscale">
                                Codice Fiscale
                            </div>
                            <div class="slds-truncate" title="Codice Fiscale"></div>
                        </li>
                        <li class="slds-page-header__detail-block">
                            <div class="slds-text-title slds-truncate" title="Partita Iva">Partita Iva</div>
                            <div class="slds-truncate" title="Partita Iva"></div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </lightning:card>

    <c:hdtSupplySelection object="{!v.object}" processType="" accountId="{!v.recordId}" targetObject="Fornitura" addititionalParam=""/>
</aura:component>

    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>IF( NOT(ISBLANK( [Case].QuotationType__c))  &amp;&amp;  [Case].QuotationType__c != &apos;Analitico&apos;,
    IF(AND([Case].QuotationType__c = &apos;Predeterminabile&apos;,
            OR(
             CONTAINS([Case].DistributorName__c, &quot;E-D&quot;),
             CONTAINS([Case].Distributor__r.Name , &quot;E-D&quot;)
             )),  TODAY() + 30,
        IF(TEXT([Case].VoltageLevelRequested__c )=&quot;BT&quot;, TODAY() + 90, 
            IF(TEXT([Case].VoltageLevelRequested__c )=&quot;MT&quot;, TODAY() + 180, null)  
           ) 
        ) , null
    )
</stringValue>
            </value>
        </processMetadataValues>

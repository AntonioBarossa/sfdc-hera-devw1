<?xml version="1.0" encoding="UTF-8"?>

<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    
    <apiVersion>50.0</apiVersion>
    
    <isExposed>true</isExposed>
    
    <targets>
        
        <target>lightning__FlowScreen</target>
        
    </targets>
    
    <targetConfigs>
        
        <targetConfig targets="lightning__FlowScreen">
            
        <property name="searchLabel" type="String" label="searchLabel" description="searchLabel" role="inputOnly"/>
             
        <property name="searchVariant" type="String" label="searchVariant" description="searchVariant" role="inputOnly"/>
             
        <property name="searchPlaceholder" type="String" label="searchPlaceholder" description="searchPlaceholder" role="inputOnly"/>
             
        <property name="accountId" type="String" label="accountId" description="accountId" role="inputOnly" required="true"/>
            
        <property name="selectionType" type="String" label="selectionType" description="selectionType" role="inputOnly" required="true"/>

        <property name="nonReqSelection" type="Boolean" label="Non Required Selection" description="Allow to not select any records" role="inputOnly"/>
            
        <property name="results" type="String" label="results" description="results" role="outputOnly"/>

        <property name="cancelCase" type="Boolean" label="cancellation" description="Cancel Case" role="outputOnly" />
        <property name="enableNew" type="Boolean" label="Abilita Nuovo BP" description="Allow to create new BP" role="inputOnly"/>

        
            
        </targetConfig>
        
    </targetConfigs>
    
</LightningComponentBundle>

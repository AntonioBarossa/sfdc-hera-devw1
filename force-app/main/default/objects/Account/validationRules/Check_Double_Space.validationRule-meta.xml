<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Double_Space</fullName>
    <active>true</active>
    <errorConditionFormula>AND(OR(CONTAINS(Name, &apos;  &apos;) ,CONTAINS(Name, &apos;   &apos;) ,CONTAINS(Name, &apos;    &apos;)),IsMigrated__c=false)</errorConditionFormula>

<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Only_Number_Phone</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( Phone , &quot;^[0-9]*$&quot;))</errorConditionFormula>
    <errorDisplayField>Phone</errorDisplayField>
    <errorMessage>Il numero di telefono fisso pu√≤ contenere solo numeri</errorMessage>
</ValidationRule>

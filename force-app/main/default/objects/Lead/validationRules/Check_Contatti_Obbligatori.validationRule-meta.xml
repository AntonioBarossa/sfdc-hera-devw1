<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Contatti_Obbligatori</fullName>
    <active>true</active>
    <errorConditionFormula>And(ISBLANK(MobilePhone),ISBLANK(Email))</errorConditionFormula>
    <errorMessage>Deve essere popolato uno dei seguenti campi : “Cellulare” , “Email”</errorMessage>
</ValidationRule>
